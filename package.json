{
  "dependencies": {
    "cypress": "^14.0.1",
    "cypress-file-upload": "^5.0.8"
  },
  "name": "agilizei-study",
  "version": "1.0.1",
  "description": "Desenvolvendo um código para automação em Cypress com estudos da agilizei",
  "main": "cypress.config.js",
  "scripts": {
    "cy open": "npx cypress open",
    "cy run": "npx cypress run",
    "cy:open:cucumber": "npx cypress open --config-file cypress-cucumber.config.js",
    "cy:run:cucumber": "npx cypress run --config-file cypress-cucumber.config.js "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrSabiin/CypressStart.git"
  },
  "author": "MrSabiin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MrSabiin/CypressStart/issues"
  },
  "homepage": "https://github.com/MrSabiin/CypressStart#readme",
  "devDependencies": {
    "chance": "^1.1.12",
    "cucumber-html-reporter": "^6.0.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "faker": "^5.5.3"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/reports/cucumber-json",
      "filePrefix": "",
      "fileSufix": ".cucumber"
    }
    
  }
}
